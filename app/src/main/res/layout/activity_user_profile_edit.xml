<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context=".View.UserProfileEditActivity">

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_margin="24dp"
            android:src="@drawable/arrow_left"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"></ImageView>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/roboto_bold"
            android:text="프로필수정"
            android:textColor="@color/black"
            android:textSize="18dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"></TextView>

        <TextView
            android:id="@+id/profile_edit_complete_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_bold"
            android:text="완료"
            android:textColor="@color/colorBlue"
            app:layout_constraintBottom_toTopOf="@+id/view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.84"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.475"></TextView>

        <View
            android:id="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="5dp"
            android:background="#E3E3E3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintVertical_bias="0.03"></View>


        <EditText
            android:id="@+id/edit_nickname_et"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginHorizontal="12dp"
            android:background="@color/white"
            android:hint="닉네임"
            android:nextFocusDown="@+id/location_et"
            android:padding="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintVertical_bias="0.011"
            tools:layout_editor_absoluteX="12dp"></EditText>


        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:layout_marginHorizontal="12dp"
            android:layout_marginBottom="464dp"
            android:background="#E3E3E3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edit_age_et"
            app:layout_constraintVertical_bias="0.017"></View>

        <EditText
            android:id="@+id/edit_gender_et"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginHorizontal="12dp"
            android:background="@color/white"
            android:hint="성별(선택사항)"
            android:nextFocusDown="@+id/time_et"
            android:padding="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintVertical_bias="0.112"
            tools:layout_editor_absoluteX="16dp">

            <requestFocus />
        </EditText>


        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:layout_marginHorizontal="12dp"
            android:layout_marginBottom="528dp"
            android:background="#E3E3E3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.119"></View>

        <EditText
            android:id="@+id/edit_age_et"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginHorizontal="12dp"
            android:background="@color/white"
            android:hint="연령대(선택사항)"
            android:nextFocusDown="@+id/people_count__et"
            android:padding="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintVertical_bias="0.219"
            tools:layout_editor_absoluteX="12dp">

            <requestFocus />
        </EditText>

        <View
            android:id="@+id/view4"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:layout_marginHorizontal="12dp"
            android:background="#E3E3E3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.083"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintVertical_bias="0.102"></View>


        <EditText
            android:id="@+id/edit_address_et"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginHorizontal="12dp"
            android:background="@color/white"
            android:hint="지역(선택사항)"
            android:padding="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintVertical_bias="0.326"
            tools:layout_editor_absoluteX="12dp">

            <requestFocus />
        </EditText>

        <ImageView
            android:id="@+id/arrow_iv"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/right_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/edit_gender_et"
            app:layout_constraintEnd_toEndOf="@+id/edit_gender_et"
            app:layout_constraintTop_toTopOf="@+id/edit_gender_et"
            app:layout_constraintVertical_bias="0.55"></ImageView>


        <ImageView
            android:id="@+id/arrow_iv2"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/right_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/edit_age_et"
            app:layout_constraintEnd_toEndOf="@+id/edit_age_et"
            app:layout_constraintTop_toBottomOf="@+id/edit_gender_et"
            app:layout_constraintVertical_bias="0.538"></ImageView>

        <ImageView
            android:id="@+id/arrow_iv3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="28dp"
            android:src="@drawable/right_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/edit_address_et"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edit_age_et"
            app:layout_constraintVertical_bias="0.547"></ImageView>



        <View
            android:id="@+id/view6"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:layout_marginHorizontal="12dp"
            android:background="#E3E3E3"
            app:layout_constraintBottom_toTopOf="@+id/edit_des_et"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.333"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edit_address_et"
            app:layout_constraintVertical_bias="0.0"></View>

        <EditText
            android:id="@+id/edit_des_et"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginHorizontal="12dp"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:gravity="top"
            android:maxLength="100"
            android:hint="자기소개를 작성해주세요(선택사항, 최대100자)."
            android:padding="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.333"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edit_address_et"
            app:layout_constraintVertical_bias="0.005"></EditText>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>